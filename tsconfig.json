{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "target": "es5",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "lib": [
      "ES2015",
      "DOM",
      "DOM.Iterable"
    ],
    "noImplicitAny": true,
    "typeRoots": [
      "./node_modules/@types",
      "types"
    ],
    "strict": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true
  },
  "exclude": [
    "**/*.js"
  ]
}